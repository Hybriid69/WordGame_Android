<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="dialogcolour" parent="Base.Theme.MaterialComponents.Light.Dialog.Alert">
        <item name="android:background">#E2E6F1</item>
<!--        <item name="colorAccent">@android:color/black</item>-->
        <item name="strokeWidth">5px</item>
        <item name="android:radius">8px</item>
        <item name="strokeColor">@android:color/black</item>
        <item name="android:opacity">translucent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />


</resources>
